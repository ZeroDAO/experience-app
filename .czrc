{
  "path": "cz-conventional-changelog",
  "disableScopeLowerCase": false,
  "disableSubjectLowerCase": false,
  "maxHeaderWidth": 100,
  "maxLineWidth": 100,
  "defaultType": "",
  "defaultScope": "",
  "defaultSubject": "",
  "defaultBody": "",
  "defaultIssues": "",
  "types": {
    "feat": {
      "description": "new features",
      "title": "add"
    },
    "fix": {
      "description": "bug fix",
      "title": "fix"
    },
    "wip": {
      "description": "feature in development, commit to switch to another branch for other development",
      "title": "in progress"
    },
    "style": {
      "description": "UI related",
      "title": "UI"
    },
    "refactor": {
      "description": "code refactoring",
      "title": "refactor"
    },
    "docs": {
      "description": "document Updates",
      "title": "docs"
    },
    "build": {
      "description": "build system or package dependency updates",
      "title": "build system or package dependency updates"
    },
    "ci": {
      "description": "CI/CD",
      "title": "CI/CD"
    },
    "test": {
      "description": "test case related",
      "title": "test"
    },
    "perf": {
      "description": "perf",
      "title": "perf"
    },
    "chore": {
      "description": "updates to non-src or test files",
      "title": "updates to non-src or test files"
    },
    "revert": {
      "description": "back to the previous version",
      "title": "revert"
    }
  }
}
